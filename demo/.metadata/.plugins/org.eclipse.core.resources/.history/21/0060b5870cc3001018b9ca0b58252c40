package com.sriya.understandJPA.demo.course;

public class Course {
	private long id;
	private String name;
	private String author;
	@Override
	public String toString() {
		return "Course [id=" + id + ", name=" + name + ", author=" + author + ", getClass()=" + getClass()
				+ ", hashCode()=" + hashCode() + ", toString()=" + super.toString() + "]";
	}
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getAuthor() {
		return author;
	}
	public void setAuthor(String author) {
		this.author = author;
	}
	
	
}
