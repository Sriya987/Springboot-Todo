package com.sriya.mockito.demo.business;

import static org.junit.jupiter.api.Assertions.assertEquals;
import static org.mockito.Mockito.mock;

import org.junit.jupiter.api.Test;
class SomeBusinessMockTest {

	@Test
	void findGreatestTest() {
		DataService mock= mock(DataService.class);
		when(mock.retrieveAllData()).thenReturn(new int[]{25,15,5});
		SomeBusinessImpl business=new SomeBusinessImpl(mock);
		int res=business.findGreatest();
		assertEquals(25,res);
	}

}