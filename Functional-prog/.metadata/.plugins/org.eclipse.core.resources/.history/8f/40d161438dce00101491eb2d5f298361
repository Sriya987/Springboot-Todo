package functionalProgramming.programming;

import java.util.List;

public class FP01 {
	private static void print(int number) {
		System.out.println(number);
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		printNumbersFunctional(List.of(12,325,1421,654));
	}

	private static void printNumbersStructured(List<Integer> numbers) {
		// TODO Auto-generated method stub
		for (int num:numbers) {
			System.out.println(num);
			
		}
	}
	private static void printNumbersFunctional(List<Integer> numbers) {
		// TODO Auto-generated method stub
		numbers.stream().forEach(FP01::print);
//		for (int num:numbers) {
//			System.out.println(num);
//			
//		}
	

}
